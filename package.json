{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "prebuild": "rimraf .cache dist build public/build",
    "build": "remix build",
    "deploy": "wrangler publish -e production",
    "dev:remix": "remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch",
    "dev": "npm-run-all build --parallel \"dev:*\"",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js",
    "typecheck": "tsc",
    "generate": "drizzle-kit generate:sqlite --schema=./app/lib/schema.ts",
    "up": "drizzle-kit up:sqlite"
  },
  "dependencies": {
    "@mantine/hooks": "^6.0.10",
    "@remix-run/cloudflare": "1.16.0",
    "@remix-run/cloudflare-workers": "1.16.0",
    "@remix-run/css-bundle": "1.16.0",
    "@remix-run/react": "1.16.0",
    "axios": "^1.4.0",
    "clsx": "^1.2.1",
    "cross-env": "7.0.3",
    "drizzle-orm": "^0.25.4",
    "isbot": "3.6.10",
    "query-string": "^8.1.0",
    "radash": "^10.8.1",
    "ramda": "^0.29.0",
    "ramda-adjunct": "^4.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.4.1",
    "remix-utils": "^6.3.0",
    "slugify": "^1.6.6",
    "tailwind-merge": "^1.12.0",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "3.19.0",
    "@remix-run/dev": "1.16.0",
    "@remix-run/eslint-config": "1.16.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/ramda": "^0.29.1",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "better-sqlite3": "^8.3.0",
    "drizzle-kit": "^0.17.6",
    "eslint": "8.39.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "miniflare": "2.13.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.23",
    "postcss-import": "15.1.0",
    "postcss-import-ext-glob": "2.1.1",
    "postcss-inline-svg": "6.0.0",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.2.8",
    "rimraf": "5.0.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4",
    "wrangler": "2.18.0"
  },
  "engines": {
    "node": ">=16.13"
  }
}
